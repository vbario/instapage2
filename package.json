  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "browser": true,
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.vue$": "vue-jest",
      ".*\\.(js|jsx)?$": "babel-jest",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      ".*\\.(css|scss|less)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      ".*\\.(css|scss)$": "<rootDir>/src/tests/__mocks__/styleMock.js",
      "^firebase$": "<rootDir>/src/tests/__mocks__/firebase.js"
    }
  }
