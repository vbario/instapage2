{
    "manifest_version": 2,
    "key": "",  
    "name": "Mighty Cart",
    "version": "1.0",
    "description": "Your anywhere shopping cart",
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "css": ["mighty-cart.css"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ],
    "permissions": [
        "identity",
        "http://localhost:5001/*",
        "contextMenus",
        "cookies",
        "history",
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
    ],
    "browser_action":{
        "default_popup": "index.html"
    },
    "options_page": "options.html",
    "background": {
        "persistent": true,  /* can I put a comment here?  cool :-) "persistent":false is supposedly incompatible with chrome.webRequest. */
        "scripts": [
            "EXACT.js",
            "background.js"
        ]
    },
    "web_accessible_resources": [
        "images/cart-icon.png",
        "images/buy-now.png"
    ]
}